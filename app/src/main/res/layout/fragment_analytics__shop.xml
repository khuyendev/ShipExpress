<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.shipexpress.shipexpress.Shop.Analytics_Shop_Fragment">

    <!-- TODO: Update blank fragment layout -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="250dp"
            android:id="@+id/lnnnshop"
            android:orientation="horizontal" >
            <com.github.mikephil.charting.charts.PieChart
                android:id="@+id/chartshop"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </LinearLayout>

        <TextView
            android:text="Tổng thu nhập:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lnnnshop"
            android:layout_centerHorizontal="true"
            android:id="@+id/txtThunhapp" />

        <TextView
            android:text="Tổng phí ship:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txtThunhapp"
            android:layout_centerHorizontal="true"
            android:id="@+id/txtPhiship" />

    </RelativeLayout>

</FrameLayout>
